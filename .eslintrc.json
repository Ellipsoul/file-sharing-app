{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc": 0,
        "new-cap": 0,
        "object-curly-spacing": 0,
        "quote-props": 0,
        "no-unused-vars": 0,
        "max-len": ["error",{"code": 100, "ignoreUrls": true}],
        "react/react-in-jsx-scope": 0
    }
}
